  <div data-system-type="metric" class="container">
    
    <div class="weather-current">
      
      <div class="weather-current__location">
        <h2 data-city class="capital-text">Unknown</h2>
        <p class="capital-text--sub"><span data-admin-division>Unknown</span>, <span data-country>?</span></p>
      </div>

      <div class="weather-current__temperature current-temperature">
        <p class="current-temperature__avg-temp"><span data-current-avg-temp>?</span><span data-unit-temp>°C</span></p>
        <p class="current-temperature__min-temp capital-text--sub">
          <span class="material-symbols-outlined capital-text--sub">arrow_downward</span>
          <span class="capital-text--sub"><span data-current-min-temp>?</span><span data-unit-temp>°C</span></span>
        </p>
        <p class="current-temperature__max-temp capital-text--sub">
          <span class="material-symbols-outlined capital-text--sub">arrow_upward</span>
          <span class="capital-text--sub"><span data-current-max-temp>?</span><span data-unit-temp>°C</span></span>
        </p>
      </div>

      <div class="weather-current__condition current-condition">
        <div class="current-condition__illustration">
          <img data-current-img src="/img/unknown-weather.svg" alt="">
        </div>
        <div data-current-condition class="current-condition__description capital-text">?</div>
      </div>

      <div class="weather-current__details details">
        <div class="details__item">
          <!-- <span class="material-symbols-outlined">mood</span> -->
          <h3 class="capital-text--sub">Feelslike</h3>
          <span><span data-feelslike>?</span><span data-unit-temp>°C</span></span>
        </div>

        <div class="details__item">
          <!-- <span class="material-symbols-outlined">air</span> -->
          <h3 class="capital-text--sub">Wind</h3>
          <span><span data-wind>?</span> <span data-unit-speed>km/h</span></span>
        </div>

        <div class="details__item">
          <!-- <span class="material-symbols-outlined">humidity_mid</span> -->
          <h3 class="capital-text--sub">Humidity</h3>
          <span data-humidity>?</span>
        </div>

        <div class="details__item">
          <!-- <span class="material-symbols-outlined">water</span> -->
          <h3 class="capital-text--sub">Rain</h3>
          <span><span data-rain>?</span> <span>%</span></span>
        </div>
      </div>

    </div>

