      <div data-tab="daily" class="tabs-container__tab weather-daily">
        <div class="weather-daily__day day">
          <h3 data-day class="day__title">Day ?</h3>
          
          <div class="day__item">
            <img data-daily-img src="/img/unknown-weather.svg" alt="">
            <span data-daily-condition>Unknown</span>
          </div>
          
          <div class="day__item">
            <h4>Avg. Temp</h4>
            <span><span data-daily-avg-temp>?</span><span data-unit-temp>°C</span></span>
          </div>

          <div class="day__item">
            <h4>Min. Temp</h4>
            <span><span data-daily-min-temp>?</span><span data-unit-temp>°C</span></span>
          </div>

