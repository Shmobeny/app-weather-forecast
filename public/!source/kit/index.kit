      <div data-tab="settings" class="tabs-container__tab weather-settings">
        <div class="weather-settings__container weather-settings__container--search">
          <p class="weather-settings__header capital-text">Weather for:</p>
          <div class="weather-settings__wrapper">
            <input data-search-field data-prev-value="" class="weather-settings__search-field capital-text--sub" type="text" name="city=search" id="search" placeholder="Enter your city here">
            <button data-settings-button="search" class="weather-settings__search-button capital-text--sub">
              <span class="material-symbols-outlined">
                travel_explore
              </span>
            </button>
            <div data-search-suggestions class="weather-settings__cities-array cities-array">
              <div></div> 
              <!-- generate suggestions here -->
            </div>
          </div>
        </div>

        <div class="weather-settings__container weather-settings__container--radio">
          <p class="weather-settings__header capital-text">System of Measures</p>
          <input data-settings-radio class="weather-settings__radio" type="radio" name="measurement-system" id="metric" checked>
          <label data-settings-button="system-metric" class="weather-settings__radio--label" for="metric">Metric</label>
          <input data-settings-radio class="weather-settings__radio" type="radio" name="measurement-system" id="eng">
          <label data-settings-button="system-eng" class="weather-settings__radio--label" for="eng">Imperial</label>
        </div>

        <div class="weather-settings__copyright">
          <a href="https://www.weatherapi.com/">Powered by WeatherAPI.com</a>
        </div>
      </div>
    </div>

    <div data-loader data-loader-status="pending" class="loader">
      <div data-loader-container="icons" class="loader__container-icons">
        <span data-loader-icon="pending" class="loader__icon loader__icon--pending material-symbols-outlined">sync</span>
        <span data-loader-icon="error" class="loader__icon loader__icon--error loader__icon--hidden material-symbols-outlined">sync_problem</span>
        <span data-loader-icon="search-fail" class="loader__icon loader__icon--search-fail loader__icon--hidden material-symbols-outlined">not_listed_location</span>
      </div>
      <div data-loader-container="texts" class="loader__container-texts">
        <span data-loader-text="pending" class="loader__text">Fetching data...</span>
        <span data-loader-text="error" class="loader__text loader__text--error loader__text--hidden">Can't get data</span>
        <span data-loader-text="search-fail" class="loader__text loader__text--search-fail loader__text--hidden">Unknown place!</span>
      </div>
